{
  "name": "alfasou.github.io",
  "version": "1.0.0",
  "description": "site pessoal/cart√£o virtual com links sociais e profissionais",
  "main": "index.js",
  "scripts": {
    "prepare": "husky || true",
    "build:ts": "tsc",
    "watch:ts": "tsc --watch",
    "build:css": "sass assets/sass:assets/css",
    "watch:css": "sass --watch assets/sass:assets/css",
    "watch": "concurrently \"npm run watch:ts\" \"npm run watch:css\"",
    "start": "node index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alfasou/alfasou.github.io.git"
  },
  "keywords": [],
  "author": "Andressa de Faria Souza",
  "license": "MIT-0",
  "bugs": {
    "url": "https://github.com/alfasou/alfasou.github.io/issues"
  },
  "homepage": "https://github.com/alfasou/alfasou.github.io#readme",
  "devDependencies": {
    "@eslint/js": "^9.6.0",
    "@types/node": "^20.14.9",
    "concurrently": "^8.2.2",
    "eslint": "^9.6.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.7.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "prettier": "3.3.2",
    "sass": "^1.77.6",
    "typescript": "^5.5.2",
    "typescript-eslint": "^7.15.0"
  },
  "overrides": {
    "eslint": "^9.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*": [
      "npx eslint **/*.ts **/*.js",
      "prettier --write",
      "git add"
    ]
  }
}
